# TOS Network - Environment Variables Example
# Copy this file to .env and modify the values as needed

# Git commit hash (auto-detected if not set)
TOS_COMMIT_HASH=

# Network configuration
TOS_NETWORK=mainnet

# Logging level
RUST_LOG=info

# Miner configuration
TOS_MINER_ADDRESS=your_wallet_address_here
TOS_MINER_THREADS=4

# AI Miner configuration
TOS_AI_MINER_ADDRESS=your_ai_wallet_address_here
TOS_AI_MODEL_PATH=/models

# Docker registry (for pushing images)
DOCKER_REGISTRY=ghcr.io/tos-network

# Database configuration
TOS_DATA_DIR=/var/run/tos/data
TOS_CONFIG_DIR=/var/run/tos/config

# API configuration
TOS_API_HOST=0.0.0.0
TOS_API_PORT=8080

# P2P configuration
TOS_P2P_PORT=2125
TOS_RPC_PORT=2126

# Performance tuning
CARGO_BUILD_JOBS=8
CARGO_TARGET_DIR=/tmp/cargo-target